<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@MainStyle.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.SolvedController">
    <children>
        <BorderPane prefHeight="200.0" prefWidth="200.0" />
        <ImageView fitHeight="157.0" fitWidth="413.0" layoutX="94.0" layoutY="30.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@Images/logo.png" />
            </image>
        </ImageView>
      <ImageView fitHeight="184.0" fitWidth="348.0" layoutX="238.0" layoutY="207.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Images/endgame.png" />
         </image>
      </ImageView>
      <Button layoutX="14.0" layoutY="279.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="219.0" text="New Game" onAction="#newGame" />
      <Button layoutX="14.0" layoutY="329.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="219.0" text="Exit Game" onAction="#exitGame" />
    </children>
</AnchorPane>
